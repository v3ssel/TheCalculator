CDTO = The\ Calculator

all: install dist check

install: build

build:
	cd ${CDTO} && qmake6 The\ Calculator.pro && $(MAKE) install -j6
	cd ${CDTO} && $(MAKE) clean
	rm -rf ${CDTO}/The\ Calculator.app/ ${CDTO}/.qmake.stash
	mv ${HOME}/${CDTO}/The\ Calculator.app/ ~/Desktop/
	/usr/local/Cellar/qt/6.2.3/bin/macdeployqt ~/Desktop/The\ Calculator.app

open:
	open ~/Desktop/The\ Calculator.app/

uninstall:
	cd ${CDTO} && qmake6 The\ Calculator.pro && $(MAKE) uninstall
	rm -rf ~/Desktop/The\ Calculator.app

clean:
	cd ${CDTO} && qmake6 The\ Calculator.pro && $(MAKE) clean
	rm -rf ${DISTDIR}.tar.gz ./report
	rm -rf ${CDTO}/The\ Calculator.app/ ${CDTO}/.qmake.stash

check:
	clang-format -style=Google -dump-config > .clang-format
	clang-format -n ${CDTO}/CalculatorSource/*
	clang-format -n ${CDTO}/Credit/*
	clang-format -n ${CDTO}/Deposit/*
	clang-format -n ${CDTO}/main.cpp
	rm -f .clang-format


dist: distdir
	(cd `dirname $(DISTDIR)` && tar -cf $(DISTNAME).tar $(DISTNAME) && gzip -9f $(DISTNAME).tar) && rm -f -r $(DISTDIR)

distdir:
	mkdir -p $(DISTDIR)
	cp -p $(DIST) $(DISTDIR)/
	cp -p /usr/local/share/qt/mkspecs/features/data/dummy.cpp $(DISTDIR)/
	cp -p The\ Calculator/CalculatorSource/stack.h The\ Calculator/CalculatorSource/calc.h The\ Calculator/Credit/credit.h The\ Calculator/Deposit/deposit.h The\ Calculator/Graphics/qcustomplot.h $(DISTDIR)/
	cp -p The\ Calculator/CalculatorSource/stack.c The\ Calculator/CalculatorSource/calc.cpp The\ Calculator/Credit/credit_calc.c The\ Calculator/Credit/credit.cpp The\ Calculator/Deposit/deposit_calc.c The\ Calculator/Deposit/deposit.cpp The\ Calculator/Graphics/qcustomplot.cpp The\ Calculator/main.cpp $(DISTDIR)/
	cp -p The\ Calculator/CalculatorSource/calc.ui The\ Calculator/Credit/credit.ui The\ Calculator/Deposit/deposit.ui $(DISTDIR)/
